{
  "manifest_version": 2,

  "name": "TabForest",
  "description": "",
  "version": "1.0",
  "background": {
    "scripts": ["background.js"]
  },
  "options_ui": {
    "page": "options.html",
    "chrome_style": true,
    "open_in_tab": true
  },
  "browser_action": {
    "default_icon": "icon.png"
  },
  "permissions": [
    "tabs",
    "activeTab",
    "storage"
  ]
}